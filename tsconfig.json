{
    "compilerOptions": {
      "sourceMap": true,
      "target": "es5",
      "jsx": "react",
      "module": "es6",
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "declaration": false,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "strict": true,
      "outDir": "dist",
      "lib": ["es6", "es7", "dom"],
      "baseUrl": "ts",
    },
    "exclude": ["dist", "build", "node_modules"]
  }
  